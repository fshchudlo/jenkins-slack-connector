{
    "parser": "@typescript-eslint/parser",
    "env": {
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
    },
    "rules": {
        "no-console": [
            0
        ],
        "strict": [
            "error",
            "global"
        ]
    },
    "overrides": [
        {
            "files": ["*.ts"],
            "extends": [
                "plugin:@typescript-eslint/recommended",
                "prettier"
            ],
            "rules": {
                "@typescript-eslint/no-explicit-any": "off"
            }
        }
    ]
}